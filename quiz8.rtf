Sub CreateColumns()\
    Dim ws As Worksheet\
    Dim i As Integer\
\
  \
    Set ws = ThisWorkbook.Sheets("Sheet1")\
    \
    \
    ws.Cells.Clear\
    \
    \
    ws.Cells(1, 1).Value = "Random 1"\
    ws.Cells(1, 2).Value = "Random 2"\
    \
    \
    For i = 2 To 51\
        ws.Cells(i, 1).Value = Rnd() * 50\
        ws.Cells(i, 2).Value = 50 + Rnd() * 50\
    Next i\
End Sub\
