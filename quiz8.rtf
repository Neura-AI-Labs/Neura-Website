Sub ReadThenWriteResultant()\
    Dim ws As Worksheet\
    Dim i As Integer\
    Dim random1 As Single\
    Dim random2 As Single\
    Dim resultant As Single\
\
    \
    Set ws = ThisWorkbook.Sheets("Sheet1")\
    \
    \
    ws.Cells(1, 3).Value = "Resultant"\
    \
    \
    For i = 2 To 51\
        random1 = ws.Cells(i, 1).Value\
        random2 = ws.Cells(i, 2).Value\
        resultant = Sqr((random1 ^ 2) + (random2 ^ 2))\
        ws.Cells(i, 3).Value = resultant\
    Next i\
End Sub\
